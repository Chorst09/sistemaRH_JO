{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "src/app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/empresas/:path*",
      "destination": "/companies/:path*"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "buildCommand": "rm -rf src/app/\\(app\\)/empresas/ 2>/dev/null || true && npm run build"
}